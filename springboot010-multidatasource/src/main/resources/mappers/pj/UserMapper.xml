<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wendy.springboot.multi.datasource.pj.dao.UserMapper">

    <select id="getUserById" resultType="com.wendy.springboot.multi.datasource.pj.domain.User" parameterType="integer" >
        SELECT id,`name` FROM `user` WHERE id=#{userId}
    </select>

</mapper>

